..if null - nothing\r\n      const name = input.value\r\n\r\n      this.model.set({ name})\r\n    }\r\n  }\r\n\r\n  onSetAgeClick = (): void => {\r\n    this.model.setRandomAge()\r\n  }\r\n  /* onSetAgeClick(): void {\r\n    this.model.setRandomAge() //error: cannot read properties of undefined\r\n  } */\r\n\r\n  onHeaderHover(): void {\r\n    console.log('hovered')\r\n  }\r\n\r\n  template(): string {\r\n    return `\r\n      <div>\r\n        <input placeholder=\"${this.model.get('name')}\"/>\r\n        <button class=\"set-name\">Change name</button>\r\n        <button class=\"set-age\">Set random age</button>\r\n        <button class=\"save-model\">Save user </button>\r\n      </div>\r\n    `\r\n  }\r\n}\r\n\r\n      //template element is a type of HTML element  <template></template>; \r\n      //turn some string into some actual HTML that can be stuck into the DOM"},"lineCount":null}},"error":null,"hash":"570072afd5b1b1763cd2ad7577738183","cacheData":{"env":{}}}